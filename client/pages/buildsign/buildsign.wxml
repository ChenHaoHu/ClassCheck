<view class="page__hd">
  <view class="page__title">创建签到</view>
  <view class="page__desc">发起人编号请联系管理员</view>
</view>
<view class="weui-cells__title">发起人编号</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder="填写你本人身份编号" bindinput='changinput' id='id' />
    </view>
  </view>
</view>

<view class="weui-cells__title">发起人密匙</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder="填写密匙" bindinput='changinput' type='password' id='pass' />
    </view>
  </view>
</view>

<view class="weui-cells__title">发起内容</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder="填写课程内容地点" bindinput='changinput'  id='content'/>
    </view>
  </view>
</view>

<view class="weui-cells__title">有效时间</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">
      <input class="weui-input" placeholder="分钟为单位" bindinput='changinput' type='number' id='time' />
    </view>
  </view>
</view>

<div bindtap="tobuild"  size="default"  class='go'>我要创建</div>